generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ship {
  pk             Int    @id @default(autoincrement())
  shipSymbol     String @unique
  systemSymbol   String
  waypointSymbol String
  navStatus      String
  fuelCurrent    Int
  fuelCapacity   Int
  cargoCapacity  Int
  cargoOnboard   Int    @default(0)
  cooldown       Int    @default(0)
  routine        String @default("")
}
